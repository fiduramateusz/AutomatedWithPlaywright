{
  "name": "playwright-udemy",
  "version": "1.0.0",
  "description": "Playwright for automated software testing ",
  "main": "index.js",
  "scripts": {
    "playwright": "npx playwright test",
    "test:chrome": "playwright test --config=playwright.config.ts --project=Chromium",
    "test:firefox": "playwright test --config=playwright.config.ts --project=Firefox",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "playwright test --config=e2e.config.ts --project=Chromium",
    "test:api": "playwright test --config=api.config.ts --project=Chromium --report=list"
  },
  "author": "Mateusz Fidura",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.22.2"
  }
}
